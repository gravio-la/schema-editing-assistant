{
  "name": "graviola-agent-tools",
  "private": true,
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev:server": "bun --filter vercel-schema-agent-server dev",
    "dev:storybook": "bun --filter storybook dev",
    "build": "bun --filter '*' build",
    "typecheck": "bun --filter '*' typecheck",
    "release:all": "bun --filter '@graviola/*' run release"
  }
}
